{
  "name": "Markdown Container",
  "containerEnv": {
    "MARKDOWN_TEMPLATE_PATH": "/.templates",
    "MARKDOWN_TEMPLATE_NAME": "template.md"
  },
  "customizations": {
    "codespaces": {
      "openFiles": [
        "sample.md",
        "draft.md"
      ],
      "files.exclude": [
        "**/.devcontainer",
        "**/github",
        "**/templates"
      ]
    }
  }
}
